@page "{handler?}/{id?}"
@model Clean.UI.Pages.Nazam.Evaluation.NazimProfile.NazimProfileModel
@{
    ViewData["Title"] = "NazimProfile";
}
<div class="panel panel-flat">
    @Html.AntiForgeryToken()
    <div class="panel-heading">
        <h1 class="panel-title">شهرت ناظم</h1>
    </div>
    <div class="panel-body" style="padding-bottom:5px;">
        <form id="dv_Nazam_Evaluation_NazimProfile" class="form main-form page-component" type="form" prefix="ux">
            <fieldset class="content-group">
                <legend class="text-bold">بخش ارزیابی ناظمین </legend>
                <input type="hidden" id="uxbid" />
                <div class="row">
                    <div class="col-lg-10">
                        <div class="row">
                            <div class="col-md-3 col-sm-12 col-xs-12 ">
                                <div class="form-group has-feedback has-feedback-left">
                                    <label class="text-bold">
                                        کود
                                    </label>
                                    <input id="uxcode" name="uxCode" type="text" class="form-control search" />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-3 col-sm-12 col-xs-12 ">
                                <div class="form-group has-feedback has-feedback-left">
                                    <label class="text-bold">
                                        نام
                                        <span class="text-danger">&nbsp;*</span>
                                    </label>
                                    <input id="uxfirstname" name="uxFirstName" type="text" class="form-control search" lang="ps" required />
                                    <div class="form-control-feedback">
                                        <i class="icon-search4 text-size-base"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-12 col-xs-12 ">
                                <div class="form-group has-feedback has-feedback-left">
                                    <label class="text-bold">
                                        تخلص
                                        <span class="text-danger">&nbsp;*</span>
                                    </label>
                                    <input id="uxlastname" name="uxLastName" type="text" class="form-control search" lang="ps" required />
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-12 col-xs-12 ">
                                <div class="form-group has-feedback has-feedback-left">
                                    <label class="text-bold">
                                        نام پدر
                                        <span class="text-danger">&nbsp;*</span>
                                    </label>
                                    <input id="uxfathername" name="uxFatherName" type="text" class="form-control search" lang="ps" required />
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-12 col-xs-12 ">
                                <div class="form-group has-feedback has-feedback-left">
                                    <label class="text-bold">
                                        نام پدرکلان
                                        <span class="text-danger">&nbsp;*</span>
                                    </label>
                                    <input id="uxgrandfathername" name="uxGrandFatherName" type="text" class="form-control search" lang="ps" required />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-3 col-sm-12 col-xs-12 ">
                                <div class="form-group has-feedback has-feedback-left">
                                    <label class="text-bold">
                                        نام انگلیسی
                                        <span class="text-danger">&nbsp;*</span>
                                    </label>
                                    <input id="uxfirstnameenglish" name="uxFirstNameEnglish" type="text" class="form-control" lang="en" required disabled />
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-12 col-xs-12 ">
                                <div class="form-group has-feedback has-feedback-left">
                                    <label class="text-bold">
                                        تخلص انگلیسی
                                        <span class="text-danger">&nbsp;*</span>
                                    </label>
                                    <input id="uxlastnameenglish" name="uxLastNameEnglish" type="text" class="form-control" lang="en" required disabled />
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-12 col-xs-12 ">
                                <div class="form-group has-feedback has-feedback-left">
                                    <label class="text-bold">
                                        نام پدر انگلیسی
                                        <span class="text-danger">&nbsp;*</span>
                                    </label>
                                    <input id="uxfathernameenglish" name="uxFatherNameEnglish" type="text" class="form-control " lang="en" required disabled />
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-12 col-xs-12 ">
                                <div class="form-group has-feedback has-feedback-left">
                                    <label class="text-bold">
                                        نام پدرکلان به انگلیسی
                                        <span class="text-danger">&nbsp;*</span>
                                    </label>
                                    <input id="uxgrandfathernameenglish" name="GrandFatherNameEnglish" type="text" class="form-control" required disabled />
                                </div>
                            </div>
                        </div>
                    </div>
                    @*<div class="col-lg-2 pull-right">
                            <div id="uxPhoto" class="img-uploader optional">
                            </div>
                            <input type="hidden" class="output" id="uxphotopath" />
                        </div>*@
                    <div class="col-lg-2 pull-right">
                        <div id="uxPhoto" class="img-uploader optional" data-type="photo" data-ref="uxphotopath">
                        </div>
                        <input type="hidden" class="output" id="uxphotopath" disabled />
                    </div>
                </div>
                <div class="main-form-details">
                    <div class="row">
                        <div class="col-md-3 col-sm-12 col-xs-12 ">
                            <div class="form-group has-feedback has-feedback-left">
                                <label class="text-bold">
                                    تاریخ تولد
                                    <span class="text-danger">*</span>
                                </label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="icon-calendar3"></i></span>
                                    <input type="text" id="uxbirthdate1" name="uxBirthDate1" class="form-control Shamsi" sibling="uxbirthdate" placeholder="تارخ تولد شمسی " required disabled />
                                    <input type="hidden" id="uxbirthdate" name="uxBirthDate" class="form-control Miladi" sibling="uxbirthdate1" placeholder="تارخ تولد شمسی " />
                                </div>
                            </div>
                        </div>
                        <div class="col-md-2 col-sm-12 col-xs-12 ">
                            <div class="form-group has-feedback has-feedback-left">
                                <label class="text-bold">
                                    گروپ خون
                                    <span class="text-danger">&nbsp;*</span>
                                </label>
                                <select id="uxbloodgroupid" name="uxBloodGroupID" class="select" asp-items="@Model.ListOfBloodGroups" required disabled>
                                    <option></option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-2 col-sm-12 col-xs-12 ">
                            <div class="form-group has-feedback has-feedback-left">
                                <label class="text-bold">
                                    جنسیت
                                    <span class="text-danger">&nbsp;*</span>
                                </label>
                                <select id="uxgenderid" name="uxGenderId" class="select" asp-items="@Model.ListOfGenders" required disabled>
                                    <option></option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-3 col-sm-12 col-xs-12 ">
                            <div class="form-group has-feedback has-feedback-left">
                                <label class="text-bold">
                                    حالت مدنی
                                    <span class="text-danger">&nbsp;*</span>
                                </label>
                                <select id="uxmaritalstatusid" name="uxMaritalStatusId" class="select" asp-items="@Model.ListOfMaritalStatus" required disabled>
                                    <option></option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
            </fieldset>
            <hr />
            <div class="row div-form-control main-form-details">
                <div class="col-md-12">
                    <div class="form-group actions">
                        <button type="button" class="btn btn-primary" action="search"><i class="icon-search4 position-left"></i> جستجو  </button>
                        <button type="button" class="btn btn-primary" action="new"><i class="icon-new-tab position-left"></i> صفحه جدید </button>
                    </div>
                </div>
            </div>
            <div class="form-grid main-form-details">
                <table bindonclick="true">
                    <thead>
                        <tr>
                            <th colname="code"> کود </th>
                            <th colname="firstname"> نام </th>
                            <th colname="lastname"> تخلص </th>
                            <th colname="fathername"> نام پدر </th>
                            <th colname="grandfathername"> نام پدر کلان </th>
                            <th colname="gendername"> جنسیت </th>
                            <th colname="religionName"> مذهب </th>
                            <th colname="candidateTypeName"> نوعیت کاندید </th>
                            <th colname="IsSelected"> انتخاب شده </th>
                            <th colname="IsCanceledName"> لغو شده </th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>

        </form>
    </div>

    <div class="page-sidebar page-component" type="actionmenu">
        <ul class="navigation navigation-accordion">
            @Html.Raw(@Model.SubScreens)
        </ul>
    </div>
</div>